{
  "compilerOptions": {
    "sourceMap": true,
    "module": "commonjs",
    "target": "es5",
    "lib": ["esnext", "dom", "dom.iterable"],
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitReturns": true,
    "jsx": "react",
    "allowJs": true,
    "baseUrl": "./",
    "outDir": "./dist",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "paths": {
      "assets/*": ["src/assets/*"],
      "components/*": ["src/components/*"],
      "layouts/*": ["src/layouts/*"],
      "config": ["src/config"],
      "endpoints": ["src/endpoints"],
      "hooks/*": ["src/hooks/*"],
      "media/*": ["src/media/*"],
      "modals/*": ["src/modals/*"],
      "pages/*": ["src/pages/*"],
      "propTypes/*": ["src/propTypes/*"],
      "reducers/*": ["src/reducers/*"],
      "routes": ["src/routes/index"],
      "schema/*": ["src/schema/*"],
      "services/*": ["src/services/*"],
      "store/*": ["src/store/*"],
      "storybook/*": [".storybook/*"],
      "store": ["src/store"],
      "theme/*": ["src/theme/*"],
      "util/*": ["src/util/*"],
      "theme": ["src/theme"]
    },
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx"],
  "exclude": ["node_modules", "dist", "coverage", "e2e"],
  "typeRoots": ["node_modules/@types", "@types", "*.d.ts"]
}
